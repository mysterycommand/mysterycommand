      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "react": "16.12.0",
    "react-dom": "16.12.0",
