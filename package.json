{
  "name": "mysterycommand",
  "version": "1.8.1",
  "description": "source code for my personal/portfolio website",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysterycommand/mysterycommand.git"
  },
  "keywords": [
    "source",
    "code",
    "personal",
    "portfolio",
    "website",
    "react",
    "three"
  ],
  "author": "Matt Hayes <matt@mysterycommand.com>",
  "license": "(MIT OR Apache-2.0)",
  "bugs": {
    "url": "https://github.com/mysterycommand/mysterycommand/issues"
  },
  "homepage": "https://mysterycommand.com",
  "engines": {
    "node": ">=12",
    "npm": "6.14.8",
    "yarn": "1.22.10"
  },
  "scripts": {
    "clean": "rm -rf build/ coverage/",
    "reset": "yarn clean node_modules/ && yarn install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "screenshots": "node scripts/screenshots.js",
    "test": "react-scripts test",
    "lint": "eslint 'src/**/*.@(j|t)s?(x)'",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "reactSnap": {
    "waitFor": 3000
  },
  "dependencies": {
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.7.1",
    "react-spring": "9.6.1",
    "react-three-fiber": "6.0.13",
    "three": "0.149.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "29.4.4",
    "@types/lodash": "4.14.191",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-helmet": "6.1.6",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "prettier": "2.8.3",
    "react-scripts": "5.0.1",
    "react-snap": "https://github.com/mysterycommand/react-snap#feat/shim-requestAnimationFrame",
    "semantic-release": "20.1.0",
    "semantic-release-github-pr": "6.0.1",
    "typescript": "4.9.5"
  }
}
